{
  "name": "gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "dev2": "NODE_TLS_REJECT_UNAUTHORIZED=0 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "depcheck": "depcheck",
    "typedoc": "typedoc",
    "eslint": "eslint src __tests__",
    "prettier": "prettier --cache --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@apollo/gateway": "^2.10.0",
    "@apollo/server": "^4.12.1",
    "@apollo/subgraph": "^2.10.2",
    "@js-temporal/polyfill": "0.4.4",
    "@nestjs/apollo": "^13.0.3",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.10",
    "@nestjs/graphql": "^13.0.3",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.0.10",
    "@nestjs/typeorm": "^11.0.0",
    "apollo-server": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "cfonts": "^3.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "helmet": "8.1.0",
    "js-yaml": "^4.1.0",
    "kafkajs": "^2.2.4",
    "keycloak-connect": "^26.1.1",
    "nest-keycloak-connect": "1.10.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "4.4.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "^9.11.1",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.0.0",
    "@tsconfig/node23": "23.0.0",
    "@types/chalk": "^0.4.31",
    "@types/compression": "1.7.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "4.0.9",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.16.0",
    "axios": "1.9.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "17.10.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-security-node": "1.1.4",
    "eslint-plugin-sonarjs": "0.25.1",
    "eslint-plugin-unicorn": "55.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "4.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "overrides": {
    "@nestjs/cli": {
      "typescript": "$typescript"
    },
    "eslint-plugin-jest": {
      "typescript": "$typescript"
    },
    "nest-keycloak-connect": {
      "@nestjs/common": "$@nestjs/common",
      "@nestjs/core": "$@nestjs/core",
      "rxjs": "7.8.2"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
